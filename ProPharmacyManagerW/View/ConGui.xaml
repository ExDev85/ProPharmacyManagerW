<Window x:Class="ProPharmacyManagerW.View.ConGui"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProPharmacyManagerW.View"
        mc:Ignorable="d"
        Title="Console" Height="400" Width="600" Background="Black" Foreground="White" Loaded="Window_Loaded" Icon="/ProPharmacyManagerW;component/Images/Command Prompt.ico" WindowStyle="ToolWindow" Closing="Window_Closing" MinHeight="300" MinWidth="500">
    <Grid>
        <RichTextBox x:Name="ConsoleOutPut" Background="Black" BorderBrush="{x:Null}" Foreground="Green" CaretBrush="White" PreviewKeyDown="richTextBox_PreviewKeyDown"/>
    </Grid>
</Window>
