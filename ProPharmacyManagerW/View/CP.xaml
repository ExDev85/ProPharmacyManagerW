        <Image Name="image1" Grid.Column="1" Stretch="Fill" HorizontalAlignment="Right" Width="54" Source="/ProPharmacyManagerW;component/Images/PPHM W Trans.png" Height="43" VerticalAlignment="Top" />
        <Border BorderBrush="{x:Null}" BorderThickness="1" Grid.ColumnSpan="2" Margin="109,0,49,0" Name="border1" MouseDown="border1_MouseDown" Background="#5E00688F" FlowDirection="RightToLeft" Height="43" VerticalAlignment="Top"/>
        <Frame x:Name="FFhost" Grid.Column="1" Source="Pages\AdminCP.xaml" BorderThickness="0" FlowDirection="RightToLeft" NavigationUIVisibility="Hidden" Grid.RowSpan="2" />
    </Grid>
</Window>
