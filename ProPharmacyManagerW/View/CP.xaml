        <ImageBrush ImageSource="/ProPharmacyManagerW;component/Images/BG.png" />
    </Window.Background>

    <Grid x:Name="GridPanel1" Background="{x:Null}" >
        
        <Grid.RowDefinitions>
            <RowDefinition Height="43*" />
            <RowDefinition Height="429*" />
